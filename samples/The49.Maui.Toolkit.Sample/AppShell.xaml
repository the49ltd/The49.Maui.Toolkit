<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="The49.Maui.Toolkit.Sample.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:The49.Maui.Toolkit.Sample"
    xmlns:pages="clr-namespace:The49.Maui.Toolkit.Sample.Pages">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />



    <ShellContent Title="OnClick" ContentTemplate="{DataTemplate pages:OnClickPage}" />
    <ShellContent Title="CollectionViewDefault" ContentTemplate="{DataTemplate pages:CollectionViewDefault}" />
    <ShellContent Title="CollectionViewGrouped" ContentTemplate="{DataTemplate pages:CollectionViewGrouped}" />


</Shell>
